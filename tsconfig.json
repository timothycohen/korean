{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "paths": {
      "$animations": ["src/lib/animations"],
      "$animations/*": ["src/lib/animations/*"],
      "$api": ["src/routes/api"],
      "$api/*": ["src/routes/api/*"],
      "$assets": ["src/assets"],
      "$assets/*": ["src/assets/*"],
      "$backend": ["src/backend"],
      "$backend/*": ["src/backend/*"],
      "$color": ["src/lib/color"],
      "$color/*": ["src/lib/color/*"],
      "$common": ["src/lib/common"],
      "$common/*": ["src/lib/common/*"],
      "$db": ["src/db"],
      "$db/*": ["src/db/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$number": ["src/lib/number"],
      "$number/*": ["src/lib/number/*"],
      "$routes": ["src/routes"],
      "$routes/*": ["src/routes/*"],
      "$time": ["src/lib/time"],
      "$time/*": ["src/lib/time/*"],
      "$tools": ["src/lib/tools"],
      "$tools/*": ["src/lib/tools/*"]
    },
    "types": ["vitest/globals"]
  }
}
